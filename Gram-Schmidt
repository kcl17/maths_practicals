import numpy as np

# Define the vectors of the original basis
v1 = np.array([1, 2, 3])
v2 = np.array([4, 5, 6])
v3 = np.array([7, 8, 9])

# Define the function for the Gram-Schmidt orthogonalization process
def gram_schmidt(A):
    Q, R = np.linalg.qr(A)
    return Q

# Compute the orthonormal basis
A = np.array([v1, v2, v3])
Q = gram_schmidt(A)

print(Q)
